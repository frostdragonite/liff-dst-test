{
    "version": 2,
    "name": "liff-test",
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "cleanUrls": true,
    "routes": [
      { "src": "/", "dest": "/src/index.html" },
      { "src": "/server-status", "dest": "/src/server-status.html" }
    ]
  }